{
  "name": "doc-builder",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "cachebust-dev": "gulp watch-cachebust-dev",
    "assets-dev": "gulp watch",
    "watchify": "watchify -vd -p browserify-hmr -e resources/src/main.js -o public/javascripts/build.js",
    "serve": "http-server ./dist -c 1 -a localhost",
    "dev": "npm-run-all --parallel start watchify cachebust-dev assets-dev",
    "build": "cross-env NODE_ENV=production browserify -e resources/src/main.js | uglifyjs -c warnings=false -m > public/javascripts/build.js",
    "lint": "eslint --ext .js,.vue src test/unit",
    "test": "karma start karma.conf.js"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "hbs": "~4.0.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0",
    "crossroads": "^0.12.2",
    "hasher": "^1.2.0",
    "lodash": "^4.15.0",
    "vue": "^1.0.0",
    "vue-resource": "^0.8.0",
    "vue-router": "^0.7.13",
    "vuex": "^0.6.3"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.5",
    "eslint": "^1.10.3",
    "eslint-plugin-html": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.1.0",
    "gulp-replace": "^0.5.4",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.23",
    "npm-run-all": "^1.6.0",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vinyl-source-stream": "^1.1.0",
    "vueify": "^8.5.2",
    "watchify": "^3.4.0"
  }
}
